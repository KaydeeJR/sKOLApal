<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_today">
    <fragment
        android:id="@+id/navigation_notes"
        android:name="com.janerose.skolapal.ui.notes.NotesFragment"
        tools:layout="@layout/notes_fragment" />
    <fragment
        android:id="@+id/navigation_study"
        android:name="com.janerose.skolapal.ui.study.StudyFragment"
        tools:layout="@layout/study_fragment" />
    <fragment
        android:id="@+id/navigation_today"
        android:name="com.janerose.skolapal.ui.today.TodayFragment"
        tools:layout="@layout/today_fragment" />

    <fragment
        android:id="@+id/navigation_to_do"
        android:name="com.janerose.skolapal.ui.todo.ToDoFragment"
        tools:layout="@layout/to_do_fragment" />

    <fragment
        android:id="@+id/navigation_timetable"
        android:name="com.janerose.skolapal.ui.timetable.TimeTableFragment"
        tools:layout="@layout/time_table_fragment" />
